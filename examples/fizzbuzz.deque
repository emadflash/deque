-- the classic fizzbuzz
--

-- prints the numbers from 1 to 15 and fizz, buzz and fizzbuzz

!1

!while !dup !16 !> {
    !if !dup !15 !% !0 !eq {
        !"fizzbuzz" !println  -- fizzbuzz, if number is divisible by 3 and 5
    } elif !dup !3 !% !0 !eq {
        !"fizz" !println  -- fizz
    } elif !dup !5 !% !0 !eq {
        !"buzz" !println  -- buzz
    } else {
        !dup !println
    }

    !dup !1 !+
}
